<Page x:Class="Memory_Project.PlayNav"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Memory_Project"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PlayNav">

    <Grid Background="White">
        <ComboBox HorizontalAlignment="Left" x:Name="comboHeight" Margin="359,110,0,0" VerticalAlignment="Top" Width="162" Height="63">
            <ComboBoxItem Content="2" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="3" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="4" HorizontalAlignment="Left" Width="99.2" IsSelected="True"/>
            <ComboBoxItem Content="5" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="6" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="7" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="8" HorizontalAlignment="Left" Width="99.2"/>
        </ComboBox>
        <ComboBox HorizontalAlignment="Left" x:Name="comboWidth" Margin="530,110,0,0" VerticalAlignment="Top" Width="160" Height="63">
            <ComboBoxItem Content="2" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="3" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="4" HorizontalAlignment="Left" Width="99.2" IsSelected="True"/>
            <ComboBoxItem Content="5" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="6" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="7" HorizontalAlignment="Left" Width="99.2"/>
            <ComboBoxItem Content="8" HorizontalAlignment="Left" Width="99.2"/>
        </ComboBox>
        <Button Content="New Game" Click="new_click" HorizontalAlignment="Left" Margin="142,110,0,0" VerticalAlignment="Top" Width="182" Height="63"/>
        <Button Content="Load Game" HorizontalAlignment="Left" Margin="142,208,0,0" VerticalAlignment="Top" Width="182" Height="63"/>
        <Button Content="Back" Click="back_click" HorizontalAlignment="Left" Margin="142,315,0,0" VerticalAlignment="Top" Width="182" Height="62"/>
        <TextBlock HorizontalAlignment="Left" Margin="412,80,0,0" TextWrapping="Wrap" Text="Height" VerticalAlignment="Top" Height="23" Width="57"/>
        <TextBlock HorizontalAlignment="Left" Margin="579,80,0,0" TextWrapping="Wrap" Text="Width" VerticalAlignment="Top" Height="23" Width="51"/>
        <Label Content="GAME SELECT&#xD;&#xA;" FontSize="25" FontWeight="Bold" HorizontalAlignment="Left" Margin="307,10,0,0" VerticalAlignment="Top" Height="43" Width="182"/>
    </Grid>
</Page>