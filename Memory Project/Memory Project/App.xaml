<Application x:Class="Memory_Project.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Memory_Project"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <sys:String x:Key="Theme">LOTR</sys:String>
        <sys:Boolean x:Key="MusicToggle">false</sys:Boolean>
        <sys:Boolean x:Key="FullscreenToggle">true</sys:Boolean>
        <sys:Double x:Key="Width">1280</sys:Double>
        <sys:Double x:Key="Height">720</sys:Double>
    </Application.Resources>
</Application>
